{
  "name": "docs-publisher-action",
  "version": "0.0.3",
  "description": "docs publisher action",
  "main": "lib/docs-publisher.js",
  "private": true,
  "scripts": {
    "build:old": "rm -rf dist && ncc build -o dist/docs-publisher src/docs-publisher.ts --license licenses.txt",
    "build": "tsc",
    "package": "ncc build -o dist/docs-publisher --license licenses.txt",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "release": "release-it --only-version",
    "all": "npm run format && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcoreni/docs-publisher-action.git"
  },
  "keywords": [],
  "author": "Marco Reni <reni.marco@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/github": "^5.0.0",
    "handlebars": "^4.7.7",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "@types/shelljs": "^0.8.9",
    "@vercel/ncc": "^0.29.2",
    "prettier": "^2.3.2",
    "release-it": "^14.11.5",
    "typescript": "^4.3.5"
  }
}
