{
  "hooks": {
    "after:init": ["npm run all"]
  }, 
  "git": {
    "commitMessage": "v${version}",
    "requireBranch": "main",
    "requireCommits": true
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "./scripts/release-it/moving-git-tag-plugin.js": {
      "key": "value"
    }
  }
}